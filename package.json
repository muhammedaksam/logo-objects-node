{
  "name": "@muhammedaksam/logo-objects-node",
  "version": "0.1.0",
  "description": "TypeScript client SDK and tools for Logo Objects REST Service API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:light": "jest --maxWorkers=2 --verbose=false",
    "test:serial": "jest --runInBand",
    "test:bail": "jest --maxWorkers=2 --bail",
    "test:quick": "node test-quick.js",
    "test:clients": "node test-clients.js",
    "test:validate": "pnpm test:quick && pnpm test:clients",
    "lint": "eslint src/**",
    "lint:fix": "eslint src/** --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"*.{js,ts,json,md}\"",
    "lint:check": "pnpm lint && pnpm format:check",
    "fix": "pnpm lint:fix && pnpm format",
    "clean": "rm -rf dist coverage",
    "version:show": "ts-node scripts/version-helper.ts show",
    "version:update": "ts-node scripts/version-helper.ts update",
    "version:bump": "ts-node scripts/version-helper.ts update",
    "prepublishOnly": "pnpm run test && pnpm run build"
  },
  "keywords": [
    "logo",
    "erp",
    "api",
    "client",
    "typescript"
  ],
  "author": "Muhammed Mustafa AKÅžAM <info@muhammedaksam.com.tr> (https://github.com/muhammedaksam)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muhammedaksam/logo-objects-node.git"
  },
  "bugs": {
    "url": "https://github.com/muhammedaksam/logo-objects-node/issues"
  },
  "homepage": "https://github.com/muhammedaksam/logo-objects-node#readme",
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "axios-mock-adapter": "^2.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^30.0.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "chalk": "^5.4.1 ",
    "commander": "^14.0.0",
    "fs-extra": "^11.3.0",
    "yaml": "^2.8.0"
  },
  "engines": {
    "node": ">=18",
    "pnpm": "^10.23.0"
  },
  "packageManager": "pnpm@10.23.0"
}
